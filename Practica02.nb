     RowBox[{"{", 
      RowBox[{"i", ",", " ", "aux", ",", " ", "ans"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"ans", " ", "=", " ", "False"}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Length", "[", 
          RowBox[{"A", "[", 
           RowBox[{"[", "5", "]"}], "]"}], "]"}], "==", "1"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"i", " ", "=", " ", "1"}], ",", " ", 
           RowBox[{"i", " ", "<=", " ", 
            RowBox[{"Length", "[", 
             RowBox[{"A", "[", 
              RowBox[{"[", "3", "]"}], "]"}], "]"}]}], ",", " ", 
           RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"aux", " ", "=", " ", 
             RowBox[{"Cases", "[", 
              RowBox[{
               RowBox[{"A", "[", 
                RowBox[{"[", "3", "]"}], "]"}], ",", " ", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{
                  RowBox[{
                   RowBox[{"A", "[", 
                    RowBox[{"[", "3", "]"}], "]"}], "[", 
